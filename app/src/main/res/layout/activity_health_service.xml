<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="#ff000000"
    tools:context="com.example.shojib.project_moon.health_service">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="65dp"
        android:text="RBS Record"
        android:id="@+id/healthButton"
        android:textColor="#ffffffff"
        android:backgroundTint="#ffff511b"
        android:layout_above="@+id/doctorButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/hospitalButton"
        android:layout_alignEnd="@+id/hospitalButton" />

    //klghjkh
    <Button
        android:layout_width="wrap_content"
        android:layout_height="65dp"
        android:text="My Doctors"
        android:textColor="#ffffffff"
        android:id="@+id/doctorButton"
        android:backgroundTint="#ff35ffa0"
        android:layout_above="@+id/vaccinationButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/hospitalButton"
        android:layout_alignEnd="@+id/hospitalButton" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="65dp"
        android:text="Medication"

        android:id="@+id/medicineButton"
        android:backgroundTint="#ffff763d"
        android:textColor="#ffffffff"
        android:layout_above="@+id/healthButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/hospitalButton"
        android:layout_alignEnd="@+id/hospitalButton" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="65dp"
        android:text="Vaccination"
        android:id="@+id/vaccinationButton"
        android:backgroundTint="#ffcdff56"
        android:textColor="#ffffffff"
        android:layout_alignParentLeft="true"
        android:layout_above="@+id/hospitalButton"
        android:layout_alignRight="@+id/hospitalButton"
        android:layout_alignEnd="@+id/hospitalButton" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="65dp"
        android:text="Hospital Info"
        android:id="@+id/hospitalButton"
        android:backgroundTint="#ffff082d"
        android:textColor="#ffffffff"
        android:layout_above="@+id/dietButton"
        android:layout_alignRight="@+id/dietButton"
        android:layout_alignEnd="@+id/dietButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="65dp"
        android:text="DIET"
        android:textColor="#ffffffff"
        android:id="@+id/dietButton"
        android:backgroundTint="#ff84ff65"
        android:layout_above="@+id/exitButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/exitButton"
        android:layout_alignEnd="@+id/exitButton" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="65dp"
        android:text="Exit"
        android:textColor="#ffffffff"
        android:id="@+id/exitButton"
        android:backgroundTint="#ffa7faff"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="iCare Health Service"
        android:id="@+id/iCare_health_service"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="29dp"
        android:textColor="#ff7fc0ff" />



</RelativeLayout>
